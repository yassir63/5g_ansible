apiVersion: v1
kind: ConfigMap
metadata:
  name: open5gs-config
  namespace: open5gs
data:
  slices.yaml: |
{% for s in fiveg_profile_data.slices %}
    - name: "{{ s.name }}"
      sst: "{{ s.sst }}"
      sd: "{{ s.sd | default('') }}"
      dnn: "{{ s.dnn }}"
{% endfor %}
  ues.yaml: |
{% for ue_name, ue_val in fiveg_profile_data.ues.items() %}
    {{ ue_name }}:
      imsi_suffix: "{{ ue_val.imsi_suffix }}"
      slice: "{{ ue_val.slice }}"
{% endfor %}
